{
  "name": "@looopy-ai/examples",
  "version": "1.0.0",
  "description": "RxJS-based AI agent framework implementing the A2A protocol",
  "type": "module",
  "scripts": {
    "check:exports": "pnpx ts-unused-exports ./tsconfig.json --excludePathsFromReport='index'",
    "check:types": "tsc --noEmit",
    "agentcore:server": "tsx src/agentcore-server.ts",
    "agentcore:client": "tsx src/agentcore-client.ts",
    "example": "tsx src/kitchen-sink.ts",
    "server": "tsx src/sse-server.ts",
    "client2": "tsx src/sse-client2.ts",
    "example:litellm": "tsx src/litellm-agent.ts",
    "lint": "biome check src",
    "lint:fix": "biome check --write src"
  },
  "keywords": [
    "agent",
    "ai",
    "rxjs",
    "a2a",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.6.5",
  "dependencies": {
    "@geee-be/sse-stream-parser": "^1.0.2",
    "@hono/node-server": "^1.19.6",
    "@looopy-ai/aws": "workspace:*",
    "@looopy-ai/core": "workspace:*",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.207.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/instrumentation": "^0.207.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "hono": "^4.10.5",
    "pino-pretty": "^13.1.2"
  },
  "devDependencies": {
    "@langfuse/client": "^4.4.2"
  }
}
